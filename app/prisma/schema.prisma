generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model hosts {
  rigId        String   @unique @default(uuid())
  hostname     String   @unique
  groupId      String?
  created_at   DateTime @default(now())
  last_updated DateTime @updatedAt
}
